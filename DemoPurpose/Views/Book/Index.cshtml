@model  dynamic

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <thead>
        <tr>
            <th>Customer Id</th>
            <th>Customer Name</th>
            <th>Customer Email</th>
            <th>Customer Age</th>
            <th>Customer Phone</th>
            <th>Customer Address</th>
            <th>Customer Gender</th>
    

        </tr>
    </thead>
    <tbody>
        @foreach (var cust in Model)
        {
        <tr>
            <th scope="row">@cust.Id</th>
            <td>@cust.Name</td>
            <td>@cust.Email</td>
            <td>@cust.Age</td>
            <td>@cust.Phone</td>
            <td>@cust.Address</td>
        
            <td>@cust.Gender</td>
            <td> @Html.ActionLink("Edit","UpdateCustomer",new { id = cust.Id}) </td>
            
        </tr>
        }
    </tbody>
</table>
